        attribution: '',
        maxZoom: 18
    }).addTo(map);


    NewMoscow.features.forEach(function(entry) {
        L.polygon(entry.geometry.coordinates[0]).bindPopup(generatePopup(entry)).addTo(map);
        console.log(entry);
    });

    function generatePopup (obj)
    {
        return "<b>"+obj.properties.NAME+"</b><br>"+
                    "<i>Объектов</i>: 17";
    }
</script>


