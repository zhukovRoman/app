$(function () {
    $('#employee_koef_chart').highcharts({
        credits:  {
            enabled: false
        },
        title: {
            text: 'Коэффициент текучести кадров'
        },
        xAxis: {
            categories: JSON.parse('<%= @plotXAxis.to_json.html_safe %>')
        },
        yAxis: {
            min: 0,
            title: {
                text: '%'
            }
        },
        tooltip: {
            valueSuffix: '%'
        },
        series: [{
            name: 'Неукомплектованность кадров',
            data: <%= @plotDatakNeuk.as_json%>,
            type: 'spline'
        }, {
            name: 'Коэффициент текучести кадров',
            type: 'spline',
            data: <%= @plotDatakTek.as_json%>
        }]
    });
});